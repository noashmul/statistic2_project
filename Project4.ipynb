    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "Q0U8XqHQ5QOj",
        "colab_type": "code",
        "colab": {}
      },
      "source": [
        "import pandas as pd\n",
        "import numpy as np\n",
        "import random\n",
        "from scipy import stats\n",
        "import math\n",
        "import matplotlib.pyplot as plt\n",
        "import scipy\n",
        "\n",
        "df = pd.read_csv(\"/content/framingham_heart_disease.csv\")\n",
        "\n",
        "#choose 200 samples randomly.\n",
        "random.seed(1)\n",
        "n_200 = random.sample(range(0,len(df)),200)\n",
        "df_200= df.loc[n_200]"
      ],
      "execution_count": 1,
      "outputs": []
    },
    {
      "cell_type": "markdown",
      "metadata": {
        "id": "yUtfbHdx-pf-",
        "colab_type": "text"
      },
      "source": [
        "####A"
