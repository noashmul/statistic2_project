      "source": [
        "Vald Test:\n",
        "\n",
        "$H_0 : \\mu_1 - \\mu_2 \\le 0 $\n",
        "\n",
        "$ H_1: \\mu_1 - \\mu_2 > 0 $\n",
        "\n",
        "$\n",
        "T = \\frac{\\bar{X}1 - \\bar{X}_2}{\\sqrt{\\frac{S{X_1}^2}\n",
        "{n_1}+\\frac{S_{X_2}^2}{n_2}}}\n",
        "$\n",
        "\n",
        "Reject if $ T > z_{1-\\alpha }$"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "Um95s-tAPYXK",
        "colab_type": "code",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 51
        },
        "outputId": "0635798b-96d1-48f6-84d7-31e22577ea4d"
      },
      "source": [
        "#calculate vald test\n",
        "alpha = 0.05\n",
        "quantile = stats.norm.ppf((1-alpha)) \n",
        "\n",
        "sp = np.sqrt(var_0 + var_1)\n",
        "T_vald = mean/ sp\n",
        "\n",
        "if T_vald > quantile:\n",
        "  print(\"reject H_0\")\n",
        "else:\n",
        "  print (\"don't reject H_0\")\n",
        "\n",
        "print(\"Ts: \" + str(T_vald))"
      ],
      "execution_count": 18,
      "outputs": [
        {
          "output_type": "stream",
